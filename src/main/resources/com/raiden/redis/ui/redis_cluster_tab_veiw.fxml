<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<Tab fx:id="tab" text="Untitled Tab" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.raiden.redis.ui.controller.RedisClusterTabController">
    <content>
        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="480.0" prefWidth="755.0">
            <children>
                <VBox prefHeight="450.0" prefWidth="755.0">
                    <children>
                        <Pane fx:id="searchPane" prefHeight="60.0" prefWidth="755.0">
                            <children>
                                <Label layoutX="8.0" layoutY="17.0" text="Key:" />
                                <TextField fx:id="searchKey" accessibleRole="TEXT_FIELD" layoutX="38.0" layoutY="14.0" prefHeight="23.0" prefWidth="322.0" />
                                <Button fx:id="searchButton" layoutX="378.0" layoutY="13.0" mnemonicParsing="false" prefHeight="23.0" prefWidth="70.0" text="搜索" onAction="#search"/>
                            </children>
                        </Pane>
                        <TableView fx:id="table" layoutY="60.0" prefHeight="360.0" prefWidth="755.0">
                            <columns>
                                <TableColumn fx:id="key" prefWidth="250.0" text="Key" />
                                <TableColumn fx:id="value" prefWidth="500.0" text="Value" />
                            </columns>
                        </TableView>
                        <Pane fx:id="bottomBar" prefHeight="60.0" prefWidth="755.0">
                            <children>
                                <Button layoutX="250.0" layoutY="8.0" mnemonicParsing="false" prefHeight="24.0" prefWidth="80.0" text="上一页" />
                                <Button layoutX="420.0" layoutY="8.0" mnemonicParsing="false" prefHeight="24.0" prefWidth="80.0" text="下一页" />
                            </children>
                        </Pane>
                    </children>
                </VBox>
            </children>
        </AnchorPane>
    </content>
</Tab>

<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.collections.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="970.0" prefWidth="931.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.raiden.redis.ui.controller.RedisDataTableController">
    <VBox prefHeight="970.0" prefWidth="931.0">
        <children>
            <Pane fx:id="searchPane" prefHeight="60.0" prefWidth="800.0">
                <children>
                    <Label layoutX="35.0" layoutY="22.0" text="Key:">
                  <font>
                     <Font name="System Bold" size="14.0" />
                  </font></Label>
                    <TextField fx:id="searchKey" accessibleRole="TEXT_FIELD" layoutX="78.0" layoutY="18.0" prefHeight="25.0" prefWidth="322.0" />
                    <Button fx:id="searchButton" layoutX="420.0" layoutY="18.0" mnemonicParsing="false" onAction="#search" prefHeight="25.0" prefWidth="70.0" text="搜索" />
                    <CheckBox fx:id="isFuzzySearch" layoutX="513.0" layoutY="15.0" mnemonicParsing="false" prefHeight="33.0" prefWidth="80.0" text="模糊查找" />
                </children>
            </Pane>
         <HBox prefHeight="840.0" prefWidth="800.0">
            <children>
               <TitledPane animated="false" collapsible="false" prefHeight="876.0" prefWidth="292.0" text="Key">
                 <content>
                   <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="862.0" prefWidth="310.0">
                        <children>
                           <ListView fx:id="keyList" prefHeight="816.0" prefWidth="292.0" />
                        </children>
                   </AnchorPane>
                 </content>
                  <font>
                     <Font name="System Bold" size="14.0" />
                  </font>
               </TitledPane>
               <AnchorPane prefHeight="840.0" prefWidth="630.0">
                  <children>
                     <SplitPane dividerPositions="0.1" orientation="VERTICAL" prefHeight="846.0" prefWidth="639.0">
                       <items>
                         <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="84.0" prefWidth="630.0">
                              <children>
                                 <Label layoutX="14.0" layoutY="32.0" text="类型">
                                    <font>
                                       <Font name="System Bold" size="14.0" />
                                    </font>
                                 </Label>
                                  <ComboBox fx:id="dataType" layoutX="50.0" layoutY="29.0" prefHeight="25.0" prefWidth="100.0">
                                      <items>
                                          <FXCollections fx:factory="observableArrayList">
                                              <String fx:value="string" />
                                              <String fx:value="list" />
                                              <String fx:value="set" />
                                              <String fx:value="zset" />
                                              <String fx:value="hash" />
                                          </FXCollections>
                                      </items>
                                  </ComboBox>
                                 <Button fx:id="add" layoutX="515.0" layoutY="14.0" mnemonicParsing="false" onAction="#add" prefHeight="22.0" prefWidth="100.0" text="添加" />
                                 <Button fx:id="delete" layoutX="515.0" layoutY="42.0" mnemonicParsing="false" prefHeight="22.0" prefWidth="100.0" text="删除" />
                                 <TextField fx:id="key" layoutX="209.0" layoutY="30.0" prefHeight="24.0" prefWidth="280.0" />
                                 <Label layoutX="167.0" layoutY="31.0" text="Key:">
                                    <font>
                                       <Font name="System Bold Italic" size="14.0" />
                                    </font>
                                 </Label>
                              </children>
                         </AnchorPane>
                         <AnchorPane fx:id="dataView" minHeight="0.0" minWidth="0.0" prefHeight="756.0" prefWidth="630.0" />
                       </items>
                     </SplitPane>
                  </children>
               </AnchorPane>
            </children>
         </HBox>
            <Pane fx:id="bottomBar" prefHeight="70.0" prefWidth="931.0">
                <children>
                    <Button layoutX="156.0" layoutY="18.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="80.0" text="上一页" />
                    <Button layoutX="556.0" layoutY="18.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="80.0" text="下一页" />
                    <ComboBox fx:id="pageSize" layoutX="356.0" layoutY="18.0" onAction="#search" prefHeight="25.0" prefWidth="80.0" value="20">
                        <items>
                            <FXCollections fx:factory="observableArrayList">
                                <String fx:value="20" />
                                <String fx:value="50" />
                                <String fx:value="100" />
                            </FXCollections>
                        </items>
                    </ComboBox>
                   <Text layoutX="755.0" layoutY="35.0" strokeType="OUTSIDE" strokeWidth="0.0" text="占用内存大小:">
                      <font>
                         <Font name="System Bold Italic" size="14.0" />
                      </font>
                   </Text>
                    <Text layoutX="850.0" fx:id="memoryUsage" layoutY="35.0" strokeType="OUTSIDE" strokeWidth="0.0" text="">
                        <font>
                            <Font name="System Bold Italic" size="14.0" />
                        </font>
                    </Text>
                </children>
            </Pane>
        </children>
    </VBox>
</AnchorPane>
